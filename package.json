  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "esbuild": "^0.14.39",
    "husky": "^3.0.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
