{
  "name": "timeline-viz",
  "version": "0.1.3",
  "description": "timeline-viz aims to be a pluggable embeddable timeline visualization that focuses on showing 100K+ individual events in a performant way.",
  "repository": {
    "type": "git",
    "url": "https://github.com/samccone/trace.git"
  },
  "dependencies": {
    "d3": "^5.12.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "dev": "webpack --mode=development --watch --config webpack.config.js",
    "build": "webpack --mode=production --config webpack.config.js",
    "format": "prettier --write **/*.ts"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/node": "^12.7.5",
    "husky": "^3.0.5",
    "json-loader": "^0.5.7",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-loader": "^6.1.2",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-livereload-plugin": "^2.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
